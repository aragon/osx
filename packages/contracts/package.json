{
  "name": "@aragon/osx-artifacts",
  "version": "1.2.1",
  "description": "The Aragon OSx Solidity contracts",
  "main": "dist/bundle-cjs.js",
  "module": "dist/bundle-esm.js",
  "types": "dist/bundle.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "hardhat test",
    "build": "hardhat compile",
    "build:npm": "rollup --config rollup.config.ts",
    "coverage": "hardhat coverage --solcoverjs ./.solcover.js",
    "flatten": "hardhat flatten",
    "analyze": "mythx analyze",
    "deploy": "hardhat deploy",
    "prepublishOnly": "yarn build && yarn build:npm",
    "docgen": "hardhat docgen",
    "formatting:check": "prettier '**/*.{json,sol,ts,js,md}' -c",
    "formatting:write": "prettier '**/*.{json,sol,ts,js,md}' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aragon/osx.git"
  },
  "author": "",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/aragon/osx/issues"
  },
  "homepage": "https://github.com/aragon/osx#readme",
  "devDependencies": {
    "@defi-wonderland/smock": "^2.3.4",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.5",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.4",
    "@openzeppelin/hardhat-upgrades": "^1.21.0",
    "@rollup/plugin-json": "^4.1.0",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.1.1",
    "ethereumjs-util": "^7.1.4",
    "ethers": "^5.5.1",
    "hardhat": "^2.12.7",
    "hardhat-deploy": "^0.9.26",
    "hardhat-gas-reporter": "^1.0.4",
    "ipfs-http-client": "51.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.1.1",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.0",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.8.2",
    "solidity-docgen": "^0.6.0-beta.35",
    "tmp-promise": "^3.0.3",
    "ts-node": "^8.1.0",
    "typechain": "^5.2.0",
    "typescript": "^4.4.4",
    "@ensdomains/ens-contracts": "0.0.11",
    "@opengsn/contracts": "2.2.6",
    "@openzeppelin/contracts": "4.8.1",
    "@openzeppelin/contracts-upgradeable": "4.8.1"
  }
